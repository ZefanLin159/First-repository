<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="address" class="com.lzf.pojo.Address">
        <property name="address" value="广州"/>
    </bean>

    <bean id="student" class="com.lzf.pojo.Student" name="student2">

        <!--    第一种，普通值注入，value-->
        <property name="name" value="狂神"/>

        <!--    第一种，bean注入，ref注入-->
        <property name="address" ref="address"/>
        <!--    数组注入，ref-->
        <property name="books">
            <array>
                <value>《红楼梦》</value>
                <value>《西游记》</value>
                <value>《水浒传》</value>
                <value>《三国演义》</value>
            </array>
        </property>

        <!--    List注入-->
        <property name="hobbys">
            <list>
                <value>听歌</value>
                <value>看电影</value>
                <value>敲代码</value>
            </list>
        </property>

        <property name="card">
            <map>
                <!--            entry是条目-->
                <entry key="身份证" value="4405091313133"/>
                <entry key="银行卡" value="443243242342"/>
            </map>
        </property>

        <!--    set注入-->
        <property name="games">
            <set>
                <value>英雄联盟</value>
                <value>王者荣耀</value>
            </set>
        </property>
        <!--    null-->
        <property name="wife">
            <null/>
        </property>
        <!--    properties-->
        <property name="info">
            <props>
                <prop key="学号">202121313</prop>
                <prop key="性别">男</prop>
                <prop key="username">小林</prop>
                <prop key="password">213445676</prop>
            </props>
        </property>
    </bean>
</beans>